{
    "name": "react-ts-redux",
    "version": "1.0.0",
    "description": "A React-Typescript-Redux blank template with integrated fake Backend, webpack, babel and eslint config.",
    "main": "src/index.tsx",
    "scripts": {
        "start": "run-p start:dev start:api",
        "start:dev": "webpack serve --config webpack.config.js --env env=development",
        "prestart:api": "node backend/createMockDB.js",
        "start:api": "node backend/ApiServer.js",
        "build": "webpack --config webpack.config.js --env env=production",
        "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
        "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "David teulieres",
    "license": "ISC",
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "redux": "^4.2.1",
        "redux-observable": "^2.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/runtime": "^7.20.13",
        "@redux-devtools/core": "^3.13.1",
        "@types/babel__core": "^7.20.0",
        "@types/babel__preset-env": "^7.9.2",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.10",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "babel-loader": "^9.1.2",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.7.3",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.5.0",
        "json-server": "^0.17.2",
        "npm-run-all": "^4.1.5",
